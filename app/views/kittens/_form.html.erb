<%= form_for @kitten do |form| %>
  <%= form.text_field :name, placeholder: "Name" %><br />
  <%= form.text_field :age, placeholder: "Age" %><br />
  <%= form.text_field :cuteness, placeholder: "Cuteness" %><br />
  <%= form.text_field :softness, placeholder: "Softness" %><br />
  <%= form.submit %>
<% end %>